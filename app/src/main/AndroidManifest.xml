        <activity
            android:name=".activity.PhoneRegistActivity2"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.RetrieveActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.RetrievePhoneActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.AddFlbActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.CompleteActivity"
            android:screenOrientation="portrait"/>


        <activity
