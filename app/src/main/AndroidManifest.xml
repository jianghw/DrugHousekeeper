        <activity android:name=".activity.MedicineManagerActivity"
                  android:screenOrientation="portrait"/>
        <activity android:name=".activity.MedicineSwitchActivity"
                  android:screenOrientation="portrait"/>
        <activity android:name=".activity.MessageNotificationActivity"
                  android:screenOrientation="portrait"/>
        <activity android:name=".activity.LanguageActivity"
                  android:configChanges="locale"
                  android:screenOrientation="portrait"/>

        <activity android:name=".activity.MProblemActivity"
